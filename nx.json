{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build:browser",
          "build:browser:modern",
          "build:browser:test",
          "build:npm",
          "build:npm:modern",
          "test",
          "check:size:build",
          "check:size"
        ]
      }
    }
  },
  "defaultBase": "main",
  "targetDefaults": {
    "test": {
      "inputs": ["default", "^default", "{workspaceRoot}/jest.preset.js"]
    }
  }
}
