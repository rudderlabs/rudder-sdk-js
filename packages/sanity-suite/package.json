{
  "name": "@rudderstack/analytics-js-sanity-suite",
  "version": "2.40.0",
  "private": true,
  "description": "Sanity suite for testing JS SDK package",
  "main": "./dist/testBook.js",
  "scripts": {
    "clean": "rimraf -rf ./dist && rimraf -rf ./node_modules/.cache && rimraf tsconfig.tsbuildinfo && rimraf -rf ./reports",
    "start": "npm run build:v1.1 && npm run build:v3 && npm run start:local",
    "build": "rollup -c --environment TEST_PACKAGE:local",
    "build:v1.1": "cd ../analytics-v1.1 && npm run build:browser",
    "build:integrations": "cd ../analytics-js-integrations && npm run build:browser",
    "build:v3": "cd ../analytics-js && npm run build:browser",
    "build:browser:modern": "exit 0",
    "build:browser": "exit 0",
    "build:npm": "rollup -c --environment TEST_PACKAGE:npm",
    "build:npm:modern": "exit 0",
    "build:worker": "exit 0",
    "build:package": "exit 0",
    "test": "exit 0",
    "test:ci": "exit 0",
    "check:lint": "eslint --ext .js,.ts src -f json -o reports/eslint.json || exit 0",
    "check:circular": "madge --circular --extensions js,ts src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src/testBook",
    "check:security": "npm audit --recursive --audit-level=high",
    "start:modern": "exit 0",
    "build:modern": "exit 0",
    "package": "exit 0",
    "release": "exit 0",
    "start:integrations": "npm run clean && mkdir dist && mkdir dist/local && npm run build:v1.1 && npm run build:integrations && rsync -r ../analytics-js-integrations/dist/cdn/legacy/js-integrations dist/local && npm run start:local",
    "start:local": "DEV_SERVER=true npm run build -- --watch",
    "start:cdn": "DEV_SERVER=true npm run build:cdn -- --watch",
    "start:cdn:v1.1": "DEV_SERVER=true npm run build:cdn:v1.1 -- --watch",
    "start:cdn:v1": "DEV_SERVER=true npm run build:cdn:v1 -- --watch",
    "start:cdn:v3": "DEV_SERVER=true npm run build:cdn:v3 -- --watch",
    "start:local:v3": "DEV_SERVER=true npm run build:local:v3 -- --watch",
    "start:npm": "DEV_SERVER=true npm run build:npm -- --watch",
    "build:cdn": "rollup -c --environment TEST_PACKAGE:cdn",
    "build:cdn:v1.1": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v1.1",
    "build:cdn:v1": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v1",
    "build:cdn:v3": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v3",
    "build:local:v3": "rollup -c --environment TEST_PACKAGE:local,CDN_VERSION_PATH:v3",
    "build:npm:v1.1": "CDN_VERSION_PATH=v1.1 npm run build:npm",
    "build:npm:v3": "CDN_VERSION_PATH=v3 npm run build:npm",
    "build:cdn:v1.1:staging": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v1.1,STAGING:true",
    "build:cdn:v1:staging": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v1,STAGING:true",
    "build:cdn:v3:staging": "rollup -c --environment TEST_PACKAGE:cdn,CDN_VERSION_PATH:v3,STAGING:true",
    "build:all": "npm run build:cdn:v1.1 && npm run build:cdn:v3 && npm run build:npm:v1.1 && npm run build:npm:v3 && rimraf -rf ./dist/dts",
    "build:all:staging": "npm run build:cdn:v1.1:staging && npm run build:cdn:v3:staging && rimraf -rf ./dist/dts"
  },
  "author": "RudderStack",
  "license": "MIT",
  "dependencies": {
    "@rudderstack/analytics-js": "^3.0.0",
    "deep-equal": "2.2.1",
    "object-path": "0.11.8",
    "rudder-sdk-js": "^2.40.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-arrow-functions": "7.22.5",
    "@babel/plugin-transform-object-assign": "7.22.5",
    "@babel/plugin-transform-runtime": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@commitlint/config-conventional": "17.6.7",
    "@commitlint/config-lerna-scopes": "17.6.6",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.1",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.4.3",
    "@size-limit/file": "8.2.6",
    "@size-limit/webpack": "8.2.6",
    "@types/component-emitter": "1.2.11",
    "@types/ramda": "0.29.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "assert": "2.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-hasown": "1.1.0",
    "commitizen": "4.3.0",
    "commitlint": "17.6.7",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "dotenv": "16.3.1",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "48.0.0",
    "esm": "3.2.25",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.6.1",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.6.1",
    "jest-junit": "16.0.0",
    "jest-sonar": "0.2.16",
    "jest-watch-typeahead": "2.2.2",
    "jscpd": "3.5.9",
    "lint-staged": "13.2.3",
    "madge": "6.1.0",
    "msw": "1.2.2",
    "patch-package": "7.0.2",
    "prettier": "2.8.8",
    "rollup": "3.20.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-generate-html-template": "1.7.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-polyfill-node": "0.12.0",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-typescript2": "0.35.0",
    "rollup-plugin-visualizer": "5.9.2",
    "size-limit": "8.2.6",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.1",
    "tslib": "2.6.0",
    "typescript": "4.9.5"
  },
  "overrides": {},
  "browserslist": {
    "production": [
      "defaults",
      "Edge >= 80",
      "Firefox >= 47",
      "IE >= 11",
      "Chrome >= 54",
      "Safari >= 7",
      "Opera >= 43"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 safari version"
    ]
  }
}
