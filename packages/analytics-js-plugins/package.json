{
  "name": "@rudderstack/analytics-js-plugins",
  "version": "3.0.0",
  "description": "RudderStack Javascript SDK",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js"
  },
  "types": "./index.d.ts",
  "keywords": [
    "analytics",
    "rudder"
  ],
  "author": "RudderStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-sdk-js.git",
    "directory": "packages/analytics-js-plugins"
  },
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js/blob/master/README.md",
  "scripts": {
    "clean": "rimraf -rf ./dist  && rimraf -rf ./node_modules/.cache && rimraf tsconfig.build.tsbuildinfo",
    "start": "rollup -c --watch --environment DEV_SERVER,PROD_DEBUG",
    "build": "npm run clean && npm run build:browser:modern",
    "build:browser:modern": "BROWSERSLIST_ENV=modern npm run build:browser",
    "build:browser": "rollup -c --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG,STAGING:$npm_config_staging,ENV:prod",
    "build:package": "exit 0",
    "test": "jest --watchAll=false --detectOpenHandles --forceExit --maxWorkers=50% --coverage",
    "test:ci": "jest --watchAll=false --detectOpenHandles --forceExit --maxWorkers=2 --coverage",
    "check:lint": "eslint --ext .js,.ts src -f json -o reports/eslint.json || exit 0",
    "check:size:build": "npm run clean && npm run build:browser && npm run build:package",
    "check:size": "npm run check:size:build && size-limit",
    "check:circular": "madge --circular --extensions js,ts src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src",
    "check:security": "npm audit --recursive --audit-level=high",
    "start:modern": "BROWSERSLIST_ENV=modern npm run start",
    "build:modern": "npm run build:browser:modern",
    "package": "exit 0",
    "release": "exit 0"
  },
  "engines": {
    "node": ">=v16"
  },
  "dependencies": {
    "@lukeed/uuid": "2.0.1",
    "@preact/signals-core": "1.2.3",
    "@rudderstack/analytics-js": "*",
    "crypto-js": "4.1.1",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-arrow-functions": "7.20.7",
    "@babel/plugin-transform-object-assign": "7.18.6",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/config-lerna-scopes": "17.4.2",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.0",
    "@originjs/vite-plugin-federation": "1.2.2",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.4.1",
    "@size-limit/file": "8.2.4",
    "@size-limit/webpack": "8.2.4",
    "@types/component-emitter": "1.2.11",
    "@types/crypto-js": "4.1.1",
    "@types/jest": "29.5.0",
    "@types/ramda": "0.28.23",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "assert": "2.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-hasown": "1.1.0",
    "commitizen": "4.3.0",
    "commitlint": "17.5.1",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "46.0.0",
    "esm": "3.2.25",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.5.0",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.5.0",
    "jest-junit": "15.0.0",
    "jest-sonar": "0.2.15",
    "jest-watch-typeahead": "2.2.2",
    "jscpd": "3.5.4",
    "lint-staged": "13.2.1",
    "madge": "6.0.0",
    "msw": "1.2.1",
    "patch-package": "6.5.1",
    "prettier": "2.8.7",
    "rollup": "3.20.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.23",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-generate-html-template": "1.7.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-polyfill-node": "0.12.0",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-typescript2": "0.34.1",
    "rollup-plugin-visualizer": "5.9.0",
    "size-limit": "8.2.4",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "overrides": {},
  "browserslist": {
    "production": [
      "defaults",
      "Edge >= 15",
      "Firefox >= 40",
      "IE >= 11",
      "Chrome >= 37",
      "Safari >= 7",
      "Opera >= 23"
    ],
    "modern": [
      "defaults",
      "supports es6-module-dynamic-import"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 safari version"
    ]
  }
}
