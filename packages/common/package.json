{
  "name": "@rudderstack/common",
  "version": "3.0.0",
  "description": "RudderStack Javascript SDK common code",
  "module": "index.js",
  "publishConfig": {
    "directory": "dist"
  },
  "keywords": [
    "analytics",
    "rudder"
  ],
  "author": "RudderStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-sdk-js.git",
    "directory": "packages/common"
  },
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js/blob/master/README.md",
  "scripts": {
    "clean": "rimraf -rf ./dist  && rimraf -rf ./node_modules/.cache && rimraf tsconfig.build.tsbuildinfo",
    "start": "exit 0",
    "build": "mkdir dist && rsync -r src/ dist",
    "build:browser:modern": "exit 0",
    "build:browser": "exit 0",
    "build:npm": "exit 0",
    "build:npm:modern": "exit 0",
    "build:package": "npm run clean && npm run build",
    "test": "jest --watchAll=false --detectOpenHandles --forceExit --maxWorkers=50% --coverage",
    "test:ci": "jest --watchAll=false --detectOpenHandles --forceExit --maxWorkers=2 --coverage",
    "check:lint:dev": "eslint --ext .js,.ts src",
    "check:lint": "eslint --ext .js,.ts src -f json -o reports/eslint.json || exit 0",
    "check:size:build": "npm run clean && npm run build",
    "check:size": "npm run check:size:build && size-limit",
    "check:size:json": "size-limit --json",
    "check:circular": "madge --circular --extensions js,ts src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src",
    "check:security": "npm audit --recursive --audit-level=high",
    "start:modern": "exit 0",
    "build:modern": "exit 0",
    "package": "npm run clean && npm run build",
    "release": "exit 0"
  },
  "engines": {
    "node": ">=v16"
  },
  "dependencies": {
    "@ndhoule/defaults": "2.0.1",
    "@segment/top-domain": "3.0.1",
    "crypto-js": "4.1.1",
    "get-value": "3.0.1",
    "ramda": "0.29.0",
    "rudder-component-cookie": "0.0.1",
    "storejs": "2.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/eslint-parser": "7.22.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-arrow-functions": "7.22.5",
    "@babel/plugin-transform-object-assign": "7.22.5",
    "@babel/plugin-transform-runtime": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@commitlint/config-conventional": "17.6.5",
    "@commitlint/config-lerna-scopes": "17.6.3",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.1",
    "@originjs/vite-plugin-federation": "1.2.2",
    "@preact/signals-core": "1.3.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.1",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.4.3",
    "@size-limit/file": "8.2.4",
    "@size-limit/webpack": "8.2.4",
    "@types/crypto-js": "4.1.1",
    "@types/jest": "29.5.2",
    "@types/ramda": "0.29.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "assert": "2.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-hasown": "1.1.0",
    "commitizen": "4.3.0",
    "commitlint": "17.6.5",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "dotenv": "16.1.4",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "47.0.0",
    "esm": "3.2.25",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.5.0",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.5.0",
    "jest-junit": "16.0.0",
    "jest-sonar": "0.2.16",
    "jest-watch-typeahead": "2.2.2",
    "jscpd": "3.5.9",
    "lint-staged": "13.2.2",
    "madge": "6.1.0",
    "msw": "1.2.2",
    "patch-package": "7.0.0",
    "prettier": "2.8.8",
    "rollup": "3.20.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.23",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-generate-html-template": "1.7.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-polyfill-node": "0.12.0",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-typescript2": "0.34.1",
    "rollup-plugin-visualizer": "5.9.2",
    "size-limit": "8.2.4",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "tslib": "2.5.3",
    "typescript": "4.9.5"
  },
  "overrides": {},
  "browserslist": {
    "production": [
      "defaults",
      "Edge >= 80",
      "Firefox >= 47",
      "IE >= 11",
      "Chrome >= 54",
      "Safari >= 7",
      "Opera >= 43"
    ],
    "modern": [
      "defaults",
      "supports es6-module-dynamic-import"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 safari version"
    ]
  }
}
