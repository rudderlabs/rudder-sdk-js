{
  "name": "rudder-analytics",
  "version": "2.20.0",
  "description": "",
  "main": "./dist/browser.min.js",
  "scripts": {
    "setup": "npm i",
    "test": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=50% --coverage",
    "test:ci": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=2 --coverage",
    "start": "npm run build:integration:all && rollup --config rollup-configs/rollup.sdk.js --watch --environment DEV_SERVER",
    "build:browser:dev": "rollup --config rollup-configs/rollup.sdk.js",
    "build:browser": "rollup --config rollup-configs/rollup.sdk.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG,STAGING:$npm_config_staging,ENV:prod",
    "build:browser:debug": "rollup --config rollup-configs/rollup.sdk.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG:inline,ENV:prod",
    "build:browser:bundle-size": "VISUALIZER=true npm run build:browser",
    "build:node:dev": "rollup --config rollup-configs/rollup.sdk.js --environment VERSION:$npm_package_version,NODE_ENV,NPM",
    "build:node": "rollup --config rollup-configs/rollup.sdk.js --environment VERSION:$npm_package_version,UGLIFY,ENV:prod,NODE_ENV,NPM",
    "build:worker": "rollup --config rollup-configs/rollup.worker.config.js --environment VERSION:$npm_package_version,NPM",
    "build:npm": "npm run build:node && npm run build:worker",
    "build:integration:dev": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,INTG_NAME:AdobeAnalytics",
    "build:integration:dev:cli": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,INTG_NAME:$npm_config_intg",
    "build:integration": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,ENV:prod,UGLIFY,INTG_NAME:AdobeAnalytics",
    "build:integration:cli": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,ENV:prod,UGLIFY,INTG_NAME:$npm_config_intg",
    "build:integration:all": "VISUALIZER=false STAGING=$npm_config_staging node -r esm scripts/integrationBuildScript.js",
    "build:integration:bundle-size": "VISUALIZER=true npm run build:integration",
    "build:integration:bundle-size:cli": "VISUALIZER=true npm run build:integration:cli",
    "clean": "rm -rf dist",
    "check:lint": "eslint \"utils/**/*.js\" \"metrics/**/*.js\" \"session/**/*.js\" \"service-worker/**/*.js\" \"integrations/**/*.js\" \"cookieConsent/**/*.js\" -f json -o reports/eslint.json || exit 0",
    "check:size": "npm run build:browser && npm run build:integration:all && npm run build:npm && size-limit",
    "check:circular": "madge --circular --extensions js ./src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src --threshold 5",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npm run check:size && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "npx standard-version",
    "release:github": "npx conventional-github-releaser -p angular",
    "changelog": "auto-changelog -p -t keepachangelog -u true -l false --sort-commits date-desc"
  },
  "author": "RudderStack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudderlabs/rudder-sdk-js.git"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@ndhoule/defaults": "^2.0.1",
    "@ndhoule/each": "^2.0.1",
    "@ndhoule/extend": "^2.0.0",
    "@segment/localstorage-retry": "^1.3.0",
    "@segment/loosely-validate-event": "^2.0.0",
    "@segment/top-domain": "^3.0.1",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "axios": "0.27.2",
    "axios-retry": "^3.2.4",
    "component-each": "^0.2.6",
    "component-emitter": "^1.3.0",
    "component-querystring": "^2.0.1",
    "component-url": "^0.2.1",
    "crypto-js": "3.1.9-1",
    "get-value": "^3.0.1",
    "is": "^3.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "lodash.tostring": "^4.1.4",
    "md5": "^2.3.0",
    "ms": "^2.1.3",
    "obj-case": "^0.2.1",
    "on-body": "^0.0.1",
    "rudder-component-cookie": "^0.0.1",
    "serialize-javascript": "^6.0.0",
    "storejs": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-arrow-functions": "^7.17.12",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/config-conventional": "17.2.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "7.4.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-big-lib": "^7.0.8",
    "auto-changelog": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "4.2.5",
    "commitlint": "17.2.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "esbuild": "^0.15.9",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-unicorn": "44.0.2",
    "esm": "^3.2.25",
    "dotenv": "16.0.3",
    "husky": "^8.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.1.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.1.1",
    "jest-sonar": "0.2.12",
    "jscpd": "3.5.1",
    "lint-staged": "^13.0.1",
    "madge": "5.0.1",
    "msw": "^0.47.3",
    "prettier": "^2.6.2",
    "rollup": "^2.75.3",
    "rollup-plugin-brotli": "^3.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-gzip": "^3.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-generate-html-template": "1.7.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-serve": "2.0.1",
    "size-limit": "^7.0.8",
    "standard-version": "9.5.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{json,js,md,ts}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraMode": false,
      "jiraOptional": true,
      "skipScope": true,
      "defaultType": "feat"
    }
  }
}
