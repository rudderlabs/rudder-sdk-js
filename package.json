{
  "name": "@rudderstack/analytics-js-monorepo",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "setup": "npx lerna bootstrap && lerna run build && lerna link",
    "setup:ci": "lerna bootstrap && lerna link",
    "clean": "lerna clean --yes && lerna run --concurrency 1 clean && git clean -xdf node_modules",
    "clean:cache": "lerna clean --yes && rimraf -rf ./node_modules/.cache",
    "start": "lerna run start --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "start:modern": "lerna run start:modern --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "build": "lerna run build",
    "build:browser": "lerna run build:browser",
    "build:browser:modern": "lerna run build:browser:modern",
    "build:npm": "lerna run build:npm",
    "build:npm:modern": "lerna run build:npm:modern",
    "build:worker": "lerna run build:worker",
    "build:package": "lerna run build:package",
    "build:integrations": "cd packages/analytics-v1.1 && npm run build:integration:all:modern && npm run build:integration:all",
    "test": "lerna run --concurrency 1 test",
    "test:ci": "lerna run --concurrency 1 test:ci",
    "check:lint": "lerna run check:lint",
    "check:lint:dev": "lerna run check:lint:dev",
    "check:size": "lerna run check:size --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "check:size:build": "lerna run check:size:build --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "check:size:json": "lerna run check:size:json --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "check:circular": "lerna run check:circular",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "lerna run check:duplicates",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test -- --concurrency 1 --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\" && npm run check:size:build -- --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\" && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "npx standard-version",
    "release:github": "DEBUG=conventional-github-releaser npx conventional-github-releaser -p angular --config github-release.config.js",
    "postinstall": "patch-package",
    "build:v3": "lerna run --concurrency 1 --scope @rudderstack/* build",
    "build:v1.1": "lerna run --concurrency 1 --scope rudder-sdk-js build",
    "build:sanity": "rm -rf packages/sanity-suite/dist && cd packages/sanity-suite && npm run build:all && npm run build:all:staging"
  },
  "author": "RudderStack",
  "license": "MIT",
