{
  "name": "rudder-analytics",
  "version": "2.40.4",
  "description": "",
  "main": "./dist/browser.min.js",
  "scripts": {
    "setup": "npm i && cd sanity-suite && npm i",
    "test": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/legacy/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=50% --coverage",
    "test:ci": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/legacy/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=2 --coverage",
    "start": "rollup --config rollup-configs/rollup.sdk.browser.js --watch --environment DEV_SERVER,PROD_DEBUG:inline",
    "start:all": "npm run build:integration:all && rollup --config rollup-configs/rollup.sdk.browser.js --watch --environment DEV_SERVER,PROD_DEBUG:inline",
    "build:browser:dev": "rollup --config rollup-configs/rollup.sdk.browser.js",
    "build:browser": "rollup --config rollup-configs/rollup.sdk.browser.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG,STAGING:$npm_config_staging,ENV:prod",
    "build:browser:modern": "BROWSERSLIST_ENV=modern rollup --config rollup-configs/rollup.sdk.browser.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG,STAGING:$npm_config_staging,ENV:prod",
    "build:browser:debug": "rollup --config rollup-configs/rollup.sdk.browser.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG:inline,ENV:prod",
    "build:browser:bundle-size": "VISUALIZER=true npm run build:browser",
    "build:browser:modern:bundle-size": "VISUALIZER=true npm run build:browser:modern",
    "build:node:dev": "rollup --config rollup-configs/rollup.sdk.npm.js --environment VERSION:$npm_package_version,NPM",
    "build:node": "rollup --config rollup-configs/rollup.sdk.npm.js --environment VERSION:$npm_package_version,UGLIFY,ENV:prod,NPM",
    "build:worker": "rollup --config rollup-configs/rollup.worker.config.js --environment VERSION:$npm_package_version,NPM",
    "build:npm": "npm run build:node && npm run build:worker",
    "build:integration:dev": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,INTG_NAME:AdobeAnalytics",
    "build:integration:dev:cli": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,INTG_NAME:$npm_config_intg",
    "build:integration": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,ENV:prod,UGLIFY,INTG_NAME:AdobeAnalytics",
    "build:integration:cli": "rollup --config rollup-configs/rollup.integrations.config.js --environment PROD_DEBUG,ENV:prod,UGLIFY,INTG_NAME:$npm_config_intg",
    "build:integration:all": "VISUALIZER=false STAGING=$npm_config_staging node -r esm scripts/integrationBuildScript.js",
    "build:integration:all:bundle-size": "VISUALIZER=true STAGING=$npm_config_staging node -r esm scripts/integrationBuildScript.js",
    "build:integration:all:modern": "VISUALIZER=false BROWSERSLIST_ENV=modern STAGING=$npm_config_staging node -r esm scripts/integrationBuildScript.js",
    "build:integration:all:modern:bundle-size": "VISUALIZER=true BROWSERSLIST_ENV=modern STAGING=$npm_config_staging node -r esm scripts/integrationBuildScript.js",
    "build:integration:bundle-size": "VISUALIZER=true npm run build:integration",
    "build:integration:bundle-size:cli": "VISUALIZER=true npm run build:integration:cli",
    "clean": "rm -rf dist",
    "check:lint": "eslint \"src/**/*.js\" -f json -o reports/eslint.json || exit 0",
    "check:size:build": "npm run clean && npm run build:browser && npm run build:integration:all && npm run build:npm",
    "check:size": "npm run check:size:build && size-limit",
    "check:circular": "madge --circular --extensions js ./src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src --threshold 5",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npm run check:size && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "npx standard-version",
    "release:github": "DEBUG=conventional-github-releaser npx conventional-github-releaser -p angular --config github-release.config.js",
    "sanity": "npm run build:browser && cd sanity-suite && npm run start:local",
    "sanity:cdn": "cd sanity-suite && npm run start:cdn",
    "sanity:npm": "cd sanity-suite && npm run start:npm",
    "build:sanity": "rm -rf sanity-suite/dist && cd sanity-suite && npm run build:all && npm run build:all:staging",
    "postinstall": "npx patch-package"
  },
  "author": "RudderStack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudderlabs/rudder-sdk-js.git"
  },
  "dependencies": {
