{
  "name": "rudder-analytics",
  "version": "1.32.0",
  "description": "",
  "main": "./dist/rudder-analytics.min.js",
  "scripts": {
    "setup": "npm i",
    "test": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=50% --coverage",
    "test:ci": "npm run build:browser && sed 's/var rudderanalytics/rudderanalytics/g' dist/rudder-analytics.min.js > __tests__/prodsdk.js && jest --watchAll=false --maxWorkers=2 --coverage",
    "start": "rollup --config rollup-configs/rollup.sdk.browser.js --watch --environment DEV_SERVER",
    "build:browser:dev": "rollup --config rollup-configs/rollup.sdk.browser.js",
    "build:browser": "rollup --config rollup-configs/rollup.sdk.browser.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG,STAGING:$npm_config_staging,ENV:prod",
    "build:browser:debug": "rollup --config rollup-configs/rollup.sdk.browser.js --environment VERSION:$npm_package_version,UGLIFY,PROD_DEBUG:inline,ENV:prod",
    "build:browser:bundle-size": "VISUALIZER=true npm run build:browser",
    "clean": "rm -rf dist",
    "check:lint": "eslint \"src/**/*.js\"",
    "check:lint:ci": "eslint \"src/**/*.js\" -f json -o reports/eslint.json || exit 0",
    "check:size:build": "npm run clean && npm run build:browser",
    "check:size": "npm run check:size:build && size-limit",
    "check:circular": "madge --circular --extensions js ./src || exit 0",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:duplicates": "jscpd src --threshold 5",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npm run check:size && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "npx standard-version",
    "release:github": "DEBUG=conventional-github-releaser npx conventional-github-releaser -p angular --config github-release.config.js"
  },
  "author": "RudderStack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudderlabs/rudder-sdk-js.git"
  },
