    "@babel/plugin-transform-object-assign": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@commitlint/config-conventional": "18.6.2",
    "@commitlint/config-nx-scopes": "18.6.1",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.1",
    "@jscutlery/semver": "4.2.0",
    "@nx/eslint": "18.0.5",
    "@nx/eslint-plugin": "18.0.5",
    "@nx/jest": "18.0.5",
    "@nx/js": "18.0.5",
    "@nx/workspace": "18.0.5",
    "@originjs/vite-plugin-federation": "1.3.5",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@rollup/plugin-terser": "0.4.4",
    "@size-limit/file": "11.0.2",
    "@size-limit/webpack": "11.0.2",
    "@types/component-emitter": "1.2.14",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.20",
    "@types/ramda": "0.29.10",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-hasown": "1.1.0",
    "commitizen": "4.3.0",
    "commitlint": "18.6.1",
    "core-js": "3.36.0",
    "dotenv": "16.4.5",
    "each": "2.6.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-compat": "4.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "esm": "3.2.25",
    "http-server": "14.1.1",
    "husky": "9.0.11",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-sonar": "0.2.16",
    "jest-watch-typeahead": "2.2.2",
    "join-component": "1.1.0",
    "jscpd": "3.5.10",
    "lint-staged": "15.2.2",
    "madge": "6.1.0",
    "nx": "18.0.5",
    "patch-package": "8.0.0",
    "prettier": "3.2.5",
    "rollup": "4.12.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.23",
    "rollup-plugin-external-globals": "0.9.2",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-generate-html-template": "1.7.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-polyfill-node": "0.13.0",
    "rollup-plugin-serve": "2.0.3",
    "rollup-plugin-typescript2": "0.36.0",
    "rollup-plugin-visualizer": "5.12.0",
    "serve": "14.2.1",
    "size-limit": "11.0.2",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "user-agent-data-types": "0.4.2",
    "msw": "2.2.1"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "18.0.5",
    "@nx/nx-darwin-x64": "18.0.5",
    "@nx/nx-linux-x64-gnu": "18.0.5",
    "@nx/nx-win32-x64-msvc": "18.0.5",
    "@rollup/rollup-linux-x64-gnu": "4.12.0"
  },
  "overrides": {
    "debug": "4.3.4",
    "ip": "2.0.1",
    "component-type": "2.0.0",
    "follow-redirects": "1.15.6"
  },
  "lint-staged": {
    "*.{json,js,md,ts}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./scripts/commitizen"
    }
  }
}
