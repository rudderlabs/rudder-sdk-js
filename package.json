{
  "name": "@rudderstack/analytics-js-monorepo",
  "version": "3.0.0-beta.5",
  "private": true,
  "description": "Monorepo accommodating Rudderstack Analytics JS SDK",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lerna:version": "lerna version --exact --conventional-commits --no-push -y",
    "lerna:publish": "lerna publish from-package --no-private -y --create-release github",
    "bump-version:monorepo": "npm version prerelease --git-tag-version false",
    "setup": "npm i",
    "setup:ci": "npm ci",
    "clean": "nx run-many -t clean & git clean -xdf node_modules && nx reset",
    "clean:cache": "rimraf -rf ./node_modules/.cache",
    "start": "lerna run start --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "start:modern": "lerna run start:modern --scope \"@rudderstack/analytics-js\" --scope \"@rudderstack/analytics-js-plugins\"",
    "build": "nx run-many -t build --parallel=false",
    "build:browser": "nx run-many -t build-browser --parallel=false",
    "build:browser:modern": "nx run-many -t build-browser-modern --parallel=false",
    "build:npm": "nx run-many -t build-npm --parallel=false",
    "build:npm:modern": "nx run-many -t build-npm-modern --parallel=false",
    "build:worker": "nx run-many -t build-worker --parallel=false",
    "build:package": "nx run-many -t build-package --parallel=false",
    "package": "nx run-many -t pack --parallel=false",
    "test": "nx run-many -t test",
    "test:ci": "nx run-many -t test --parallel=false --configuration=ci --runInBand --maxWorkers=1 --forceExit",
    "check:lint": "nx run-many -t check:lint --parallel=false --verbose",
    "check:lint:dev": "nx run-many -t check:lint:dev --parallel=false --verbose",
    "check:size": "nx run-many -t check:size --parallel=false --verbose",
    "check:size:build": "nx run-many -t build-check-size",
    "check:size:json": "nx run-many -t check:size:json --parallel=false --verbose",
    "check:circular": "nx run-many -t check:circular --parallel=false --verbose",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:support:modern": "NODE_ENV=modern npx browserslist --mobile-to-desktop",
    "check:duplicates": "nx run-many -t check:duplicates --parallel=false --verbose",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npm run check:size:build && npm run check:size:json && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "echo 'TODO: Use NX for this' && exit 0",
    "release:github": "echo 'TODO: Use NX for this' && exit 0",
    "postinstall": "patch-package",
    "build:integrations": "nx run-many --targets=build-browser,build-browser-modern --parallel=false --projects=@rudderstack/analytics-js-integrations",
    "build:v3": "nx run-many -t build --parallel=false --exclude=rudder-sdk-js,@rudderstack/analytics-js-sanity-suite,@rudderstack/analytics-js-integrations",
    "build:v1.1": "nx run-many -t build --parallel=false --projects=rudder-sdk-js",
    "build:sanity": "rm -rf packages/sanity-suite/dist && nx run-many --targets=build:all,build:all:staging --parallel=false --projects=@rudderstack/analytics-js-sanity-suite"
  },
  "author": "RudderStack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-js/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-js/blob/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudderlabs/rudder-sdk-js.git"
  },
